{
  "name": "zip",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "12.13.x",
    "npm": "6.14.x"
  },
  "scripts": {
    "start": "node index.js",
    "start-dev": "nodemon index.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:reset": "sequelize-cli db:migrate:undo:all && npm run migrate",
    "test": "jest --forceExit --coverage --coverageDirectory=coverage",
    "test:watch": "jest --testTimeout=10000 --forceExit --watch",
    "seed": "sequelize-cli db:seed:all",
    "pretest": "npm run migrate:reset",
    "postinstall": "npm run migrate",
    "lint": "eslint . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dayjs": "^1.8.23",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.0.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.11",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.3",
    "jest": "^25.2.4",
    "jest-runner-eslint": "^0.7.6",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "testEnvironment": "node",
  "coveragePathIgnorePatterns": [
    "/node_modules/"
  ],
  "husky": {
    "hooks": {
      "pre-push": "npm run test"
    }
  },
  "jest": {
    "watchPlugins": ["jest-runner-eslint/watch-fix"],
    "coverageReporters": [
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "functions": 30,
        "lines": 30,
        "statements": 30
      }
    },
    "projects": [
      {
        "displayName": "jest-test"
      },
      {
        "displayName": "lint-test",
        "runner": "jest-runner-eslint",
        "testMatch": [
          "<rootDir>/**/*.js"
        ]
      }
    ]
  },
  "cliOptions": {
    "fix": true,
    "ext": [".jsx", ".ts"]
  }
}
