{
  "name": "zip",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start-dev": "nodemon index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000 --forceExit --coverage",
    "testwatch": "cross-env NODE_ENV=test jest --testTimeout=10000 --forceExit --watch",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset",
    "postinstall": "npm run migrate",
    "coverage": "npm run pretest && jest --coverage --forceExit && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.0.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.11",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.2.4",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "testEnvironment": "node",
  "coveragePathIgnorePatterns": [
    "/node_modules/"
  ]
}